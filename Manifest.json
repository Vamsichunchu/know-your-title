


{
    "name": "know your title",
    "description": "an extension which shows the title",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["tabs", "activeTab"],
    "host_permissions": [
        "*://*/*"
    ],
    "background":{
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "/images/1616.jpg",
        "32": "/images/3232(1).png",
        "48": "/images/4848(1).png"
    }
}